{
    "name": "react-starter",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "clean": "rm -rf dist",
        "build": "export NODE_ENV=production && npm run clean && webpack --mode production",
        "dev": "export NODE_ENV=development && webpack-dev-server --open --host 0.0.0.0 --mode development",
        "deploy": "node tools/deploy.js"
    },
    "keywords": [
        "boilerplate",
        "react",
        "starter"
    ],
    "author": "Dave Schumaker <dave@dave.ly> (https://dave.ly)",
    "license": "MIT",
    "prettier": {
        "arrowParens": "avoid",
        "bracketSpacing": true,
        "jsxBracketSameLine": false,
        "printWidth": 100,
        "semi": true,
        "singleQuote": true,
        "tabWidth": 4,
        "trailingComma": "none"
    },
    "devDependencies": {
        "@babel/core": "7.8.7",
        "@babel/plugin-proposal-class-properties": "7.8.3",
        "@babel/plugin-transform-react-jsx": "7.8.3",
        "@babel/preset-env": "7.8.7",
        "@babel/preset-react": "7.8.3",
        "babel-eslint": "10.1.0",
        "babel-loader": "8.0.6",
        "babel-plugin-styled-components": "1.10.7",
        "copy-webpack-plugin": "5.1.1",
        "css-loader": "3.4.2",
        "eslint": "6.8.0",
        "eslint-plugin-react": "7.18.3",
        "eslint-plugin-react-hooks": "2.5.0",
        "file-loader": "5.1.0",
        "html-webpack-plugin": "3.2.0",
        "mini-css-extract-plugin": "0.9.0",
        "sftp-sync-deploy": "0.7.1",
        "svg-url-loader": "4.0.0",
        "url-loader": "3.0.0",
        "webpack": "4.42.0",
        "webpack-cli": "3.3.11",
        "webpack-dev-server": "3.10.3"
    },
    "dependencies": {
        "app-root-path": "3.0.0",
        "normalize.css": "8.0.1",
        "react": "16.13.0",
        "react-dom": "16.13.0",
        "styled-components": "5.0.1"
    }
