{
    "name": "react-starter",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "clean": "rm -rf dist",
        "build": "export NODE_ENV=production && npm run clean && webpack --mode production",
        "dev": "export NODE_ENV=development && webpack-dev-server --open --mode development",
        "deploy": "node tools/deploy.js"
    },
    "keywords": [
        "boilerplate",
        "react",
        "starter"
    ],
    "author": "Dave Schumaker <dave@dave.ly> (https://dave.ly)",
    "license": "MIT",
    "prettier": {
        "arrowParens": "avoid",
        "bracketSpacing": true,
        "jsxBracketSameLine": false,
        "printWidth": 100,
        "semi": true,
        "singleQuote": true,
        "tabWidth": 4,
        "trailingComma": "none"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "devDependencies": {
        "@babel/core": "7.11.4",
        "@babel/plugin-proposal-class-properties": "7.10.4",
        "@babel/plugin-transform-react-jsx": "7.10.4",
        "@babel/preset-env": "7.11.0",
        "@babel/preset-react": "7.10.4",
        "babel-eslint": "10.1.0",
        "babel-loader": "8.3.0",
        "babel-plugin-styled-components": "1.11.1",
        "copy-webpack-plugin": "6.0.3",
        "css-loader": "4.2.1",
        "eslint": "7.7.0",
        "eslint-plugin-react": "7.20.6",
        "eslint-plugin-react-hooks": "4.1.0",
        "file-loader": "6.0.0",
        "html-webpack-plugin": "5.5.0",
        "husky": "4.2.5",
        "mini-css-extract-plugin": "2.7.2",
        "prettier": "2.0.5",
        "pretty-quick": "3.0.0",
        "sftp-sync-deploy": "0.7.1",
        "svg-url-loader": "6.0.0",
        "url-loader": "4.1.0",
        "webpack": "5.75.0",
        "webpack-cli": "5.0.1",
        "webpack-dev-server": "3.11.0"
    },
    "dependencies": {
        "app-root-path": "3.0.0",
        "normalize.css": "8.0.1",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "styled-components": "5.1.1"
    }
}
